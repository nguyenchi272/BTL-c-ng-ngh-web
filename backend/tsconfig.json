{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 18",
    "extends": "@tsconfig/node18/tsconfig.json",
    "_version": "18.2.0",
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "module": "node16",
        "target": "es2022",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node16"
    }
}