#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/cli/esm/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/cli/esm/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/cli/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules/@graphql-codegen/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/@graphql-codegen+cli@5.0.0_@types+node@18.18.9_graphql@16.8.1_typescript@5.2.2/node_modules:/home/nguyenchi/web/project/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
